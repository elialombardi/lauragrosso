@font-face{font-family:"Nanum Gothic";src:url("/fonts/nanum-gothic/NanumGothic-Regular.ttf") format("truetype");font-weight:normal;font-style:normal}body{font-family:'Nanum Gothic', sans-serif;font-weight:100}.nav-link{color:#666}.nav-link.main-nav-link{font-size:1.3em;font-weight:700}#lightgallery{display:flex;flex-wrap:wrap;justify-content:center;gap:20px}#homeContainer{padding:0;display:flex;flex-wrap:wrap;justify-content:center;gap:20px;list-style:none}#homeContainer>li{flex-basis:350px;position:relative;cursor:pointer}#homeContainer>li img{object-fit:cover;width:100%;height:100%;vertical-align:middle;border-radius:5px;max-height:350px}#homeContainer>li .overlay{position:absolute;width:100%;height:100%;background:rgba(57,57,57,0.502);bottom:0;height:30px;transition:all 0.2s 0.1s ease-in-out;color:#fff;border-radius:5px;display:flex;align-items:center;justify-content:right;font-size:1.3em}#homeContainer>li .overlay span{text-align:right;padding-right:10px;font-style:italic}
